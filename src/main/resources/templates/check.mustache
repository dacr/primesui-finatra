{{<layout.mustache}}
  {{$title}}PrimesUI check result{{/title}}
  {{$body}}
    <h1>
      {{#value}}
         {{#value.isPrime}}
           {{num}} is the {{nth}}th prime
         {{/value.isPrime}}
         {{^value.isPrime}}
