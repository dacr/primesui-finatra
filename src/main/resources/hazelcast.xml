<?xml version="1.0" encoding="UTF-8"?>
<hazelcast
xsi:schemaLocation="http://www.hazelcast.com/schema/config
http://www.hazelcast.com/schema/config/hazelcast-config-3.0.xsd"
xmlns="http://www.hazelcast.com/schema/config"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

<!-- 
	<network>
		<join>
			<multicast enabled="true" />
		</join>
	</network>
-->

    <network>
        <join>
            <multicast enabled="false" />
            <tcp-ip enabled="false"/>
            <aws enabled="false"/>
        </join>
    </network>


  <map name="CachedValuesCache">
        <in-memory-format>OBJECT</in-memory-format>
        <max-size policy="PER_NODE">20000</max-size>
        <max-idle-seconds>600</max-idle-seconds>
        <time-to-live-seconds>3600</time-to-live-seconds>
        <eviction-policy>LRU</eviction-policy>
        <eviction-percentage>0</eviction-percentage>
        <min-eviction-check-millis>5000</min-eviction-check-millis>
        <backup-count>0</backup-count>
  </map>
  <map name="PrimesCache">
        <in-memory-format>OBJECT</in-memory-format>
        <max-size policy="PER_NODE">20000</max-size>
        <max-idle-seconds>600</max-idle-seconds>
        <time-to-live-seconds>3600</time-to-live-seconds>
        <eviction-policy>LRU</eviction-policy>
        <eviction-percentage>0</eviction-percentage>
        <min-eviction-check-millis>5000</min-eviction-check-millis>
        <backup-count>0</backup-count>
  </map>
  <map name="CountersCache">
        <in-memory-format>OBJECT</in-memory-format>
        <max-size policy="PER_NODE">10</max-size>
        <max-idle-seconds>10</max-idle-seconds>
        <time-to-live-seconds>300</time-to-live-seconds>
        <eviction-policy>LRU</eviction-policy>
        <eviction-percentage>0</eviction-percentage>
        <min-eviction-check-millis>5000</min-eviction-check-millis>
        <backup-count>0</backup-count>
  </map>
  <map name="LastCache">
        <in-memory-format>OBJECT</in-memory-format>
        <max-size policy="PER_NODE">10</max-size>
        <max-idle-seconds>10</max-idle-seconds>
        <time-to-live-seconds>300</time-to-live-seconds>
        <eviction-policy>LRU</eviction-policy>
        <eviction-percentage>0</eviction-percentage>
        <min-eviction-check-millis>5000</min-eviction-check-millis>
        <backup-count>0</backup-count>
  </map>
  <map name="FactorsCache">
        <in-memory-format>OBJECT</in-memory-format>
        <max-size policy="PER_NODE">5000</max-size>
        <max-idle-seconds>3600</max-idle-seconds>
        <time-to-live-seconds>7200</time-to-live-seconds>
        <eviction-policy>LRU</eviction-policy>
        <eviction-percentage>0</eviction-percentage>
        <min-eviction-check-millis>5000</min-eviction-check-millis>
        <backup-count>0</backup-count>
  </map>
  <map name="UlamCache">
        <in-memory-format>OBJECT</in-memory-format>
        <max-size policy="PER_NODE">500</max-size>
        <max-idle-seconds>3600</max-idle-seconds>
        <time-to-live-seconds>0</time-to-live-seconds>
        <eviction-policy>LRU</eviction-policy>
        <eviction-percentage>0</eviction-percentage>
        <min-eviction-check-millis>5000</min-eviction-check-millis>
        <backup-count>0</backup-count>
  </map>

</hazelcast>